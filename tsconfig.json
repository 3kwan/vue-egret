{
	"name": "vue-egret",
	"author": "Hsuna",
	"compilerOptions": {
		"allowJs": false,
		"sourceMap": true,
		"target": "es5",
		"jsx": "react",
		"moduleResolution": "node",
		"downlevelIteration": true,
		"emitDecoratorMetadata": false,
		"experimentalDecorators": true,
		"declaration": false,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"removeComments": true,
		"strictNullChecks": false,
		"baseUrl": ".",
		"outDir": "./types/",
		"paths": {
			"src/*": ["./src/*"]
		}
	},
	"includes": [
		"./types/*"
	],
	"exclude": [
		"dist",
		"build",
		"config",
		"scripts",
		"public",
		"node_modules",
		"libs"
	]
}
