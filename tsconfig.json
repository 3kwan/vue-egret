{
	"name": "vue-egret",
	"author": "Hsuna",
	"compilerOptions": {
		"allowJs": false,
		"sourceMap": true,
		"target": "es5",
		"jsx": "react",
		"moduleResolution": "node",
		"emitDecoratorMetadata": false,
		"experimentalDecorators": true,
		"declaration": true,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"removeComments": true,
		"strictNullChecks": false,
		"baseUrl": ".",
		"paths": {
			"src/*": ["./src/*"]
		}
	},
	"exclude": [
		"dist",
		"build",
		"config",
		"scripts",
		"public",
		"node_modules",
		"lib"
	]
}
